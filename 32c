#include<stdio.h>
int main()
{
	int n,a,m=0;
	scanf("%d",&n);
	while(n!=0)
	{
		a=n%10;
		m=m*10+a;
		n=n/10;
	}
	printf("%d",m);

}




#include <stdio.h>
int main()
{
	int n1=0, n2=1, n, n3, sum=0;
	printf("enter the range of fibonacci series: ");
	scanf("%d", &n);
	printf("the fibonacci series is: \t");
	while(n1<n)
	{
		printf("%d\t", n1);
		sum+=n1;
		n3 = n1+n2;
		n1 = n2;
		n2 = n3;
	}
	printf("\n their sum is =%d", sum);
	return 0;
}



#include<stdio.h>
int main()
{
	int n[10]={1,14,16,23,25,80};
	int i,c;
	scanf("%d",&c);
	for(i=0;i<=5;i++)
	{
		if(n[i]==c)
		{
			printf("%d",i+1);
		}
	}
}
